{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:recommended",
		"default:pinDigestsDisabled"
	],
	"packageRules": [
		{
			"matchUpdateTypes": ["patch", "digest"],
			"matchManagers": ["woodpecker"],
			"enabled": false
		}
	],
	"minimumReleaseAge": "14 days"
}
